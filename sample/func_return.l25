program MyApp { 
 func add(a: int, b: int) { 
 let sum = a + b; 
 return sum; 
 } 
  
 func square(x: int): int { 
 let result = x * x; 
 return result; 
 } 
 
 main { 
 let x = 5; 
 let y = add(x, 10); 
 let z = square(y); 
 
 if (z > 50) { 
 output(z); 
 } else { 
 output(0); 
 }; 
 output('\n', add(x,y), '\n');
 }
} 